{
    "name": "cspray/labrador-http",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cspray/phpunit-mock-objects"
        }
    ],
    "require": {
        "php": "~7.0",
        "cspray/labrador": "~0.1.1",
        "nikic/fast-route": "0.3.0",
        "symfony/http-foundation": "2.6.*",
        "filp/whoops": "1.1.5",
        "cspray/telluris": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.4.*",
        "phpunit/phpunit-mock-objects": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Cspray\\Labrador\\Http\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Cspray\\Labrador\\Http\\Test\\": ["tests/"]
        }
    }

}

{
  "name": "cspray/labrador-http",
  "require": {
    "php": "~7.2",
    "ext-json": "*",
    "amphp/http-server": "~1.0",
    "cspray/labrador": "~3.0.0-beta2",
    "league/uri-schemes": "~1.2",
    "nikic/fast-route": "~1.3",
    "monolog/monolog": "^1.24"
  },
  "require-dev": {
    "phpunit/phpunit": "~7",
    "amphp/artax": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Cspray\\Labrador\\Http\\": "src/"
    },
    "files": [
      "src/_functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Cspray\\Labrador\\Http\\Test\\": "test/"
    }
  }
}
